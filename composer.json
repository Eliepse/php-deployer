{
    "name" : "eliepse/php-deployer",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Eliepse",
            "email" : "contact@eliepse.fr"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Eliepse\\Deployer\\" : "src/"
        },
        "files" : [
            "src/helpers.php"
        ]
    },
    "autoload-dev" : {
        "psr-4" : {
            "Tests\\" : "tests/"
        }
    },
    "require" : {
        "php" : "^7.2",
        "symfony/process" : "^4.1",
        "nesbot/carbon" : "1.* || 2.*",
        "symfony/console": "^4.1",
        "psr/log": "^1.0",
        "monolog/monolog": "^1.23"
    },
    "require-dev" : {
        "phpunit/phpunit" : "7",
        "codedungeon/phpunit-result-printer" : "^0.23.3"
    }
}
